{
    "name": "pick",
    "displayName": "PickBasic",
    "description": "Pick Basic Syntax Highlighting and language support",
    "version": "1.0.0",
    "publisher": "TravisHaley",
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "pick",
            "aliases": ["Pick Basic", "pick"],
            "extensions": [".txt", ".pick", ""],
            "configuration": "./pick.configuration.json"
        }],
        "grammars": [{
            "language": "pick",
            "scopeName": "source.pick",
            "path": "./syntaxes/pick.tmLanguage"
        }]
    },
    "repository": {
        "url": "https://github.com/mallek/vscode-pick-basic-syntax-highlighter"
    }
}
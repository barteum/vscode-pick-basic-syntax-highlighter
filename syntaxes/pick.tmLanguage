	<array>
		<string>pick</string>
	</array>
	<key>name</key>
	<string>Pick Basic</string>
	<key>patterns</key>
	<array>
		<dict>
			<key>match</key>
			<string>((\;|^)\s{0,}\*).*|((\;|^)\s{0,}\!).*|\b(?i:REM)\b.*</string>
			<key>name</key>
			<string>comment.line.asterisk.pick</string>
		</dict>
		<dict>
			<key>captures</key>
			<dict>
				<key>1</key>
				<dict>
					<key>name</key>
					<string>storage.type.pick</string>
				</dict>
				<key>2</key>
				<dict>
					<key>name</key>
					<string>entity.name.function.pick</string>
				</dict>
			</dict>
			<key>comment</key>
			<string>Program and Subroutine Highlighting</string>
			<key>match</key>
			<string>(?i:SUBROUTINE|PROGRAM) ([A-Za-z_-]+)</string>
			<key>name</key>
			<string>entity.name.function.pick</string>
		</dict>
		<dict>
		    <key>match</key>
		    <string>^([A-Za-z0-9_\.])+\:|^[0-9\.]+\b|;|,</string>
	   	    <key>name</key>
		    <string>entity.name.function.pick</string>
		</dict>
		<dict>
			<key>match</key>
			<string>\b([0-9]+)\b</string>
			<key>name</key>
			<string>constant.numeric.pick</string>
		</dict>
		<dict>
			<key>match</key>
			<string>(\\(.)+\\|\'(.)+\'|\"(.)+\")</string>
			<key>name</key>
			<string>string.other.quoted-or-unquoted.pick</string>
		</dict>
		<dict>
			<key>comment</key>
			<string>Pick Clauses</string>
			<key>match</key>
			<string>\b(?i:CAPTURING|DO|END|ELSE|LOCKED|ONERR|ON ERROR|SETTING|THEN|UNTIL|WHILE)\b</string>
			<key>name</key>
			<string>keyword.other.pick</string>
		</dict>
		<dict>
			<key>comment</key>
			<string>Pick Functions 1</string>
			<key>match</key>
			<string>\b(?i:@|ABS|ACCESS|ALPHA|ASCII|ASSIGNED|CHANGE|CHAR|COL1|COL2|CONVERT|COS|COUNT|DATE|DCOUNT|DELETE|DQUOTE|DTX|EBCDIC|EREPLACE|ERROR|EXCHANGE|EXP)\b</string>
			<key>name</key>
